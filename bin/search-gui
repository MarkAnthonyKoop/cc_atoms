#!/bin/bash
# Data Search GUI - Visual interface for querying indexed data
# Uses chromadb venv for compatibility

VENV_PYTHON="$HOME/.venvs/chromadb-env/bin/python"

if [ ! -f "$VENV_PYTHON" ]; then
    echo "Error: chromadb venv not found at $VENV_PYTHON"
    exit 1
fi

exec "$VENV_PYTHON" -m cc_atoms.tools.multi_db_agent.search_gui "$@"
